$linkedData:
  term: GS18PrefixLicenceCredential
  '@id': https://w3id.org/traceability#GS18PrefixLicenceCredential
title: GS1 Prefix Licence Credential
tags:
  - eCommerce
description: >-
  A GS1 Prefix is issued by GS1 Global Office and allocated to a GS1 Member Organization or to itself for the purpose of issuing GS1 Company Prefix or GS1 identification key licenses.
type: object
properties:
  '@context':
    type: array
    readOnly: true
    const:
      - https://www.w3.org/2018/credentials/v1
      - https://ref.gs1.org/gs1/vc/licence-context/
    default:
      - https://www.w3.org/2018/credentials/v1
      - https://ref.gs1.org/gs1/vc/licence-context/
    items:
      type: string
      enum:
        - https://www.w3.org/2018/credentials/v1
        - https://ref.gs1.org/gs1/vc/licence-context/
  type:
    type: array
    readOnly: true
    const:
      - VerifiableCredential
      - GS18PrefixLicenceCredential
    default:
      - VerifiableCredential
      - GS18PrefixLicenceCredential
    items:
      type: string
      enum:
        - VerifiableCredential
        - GS18PrefixLicenceCredential
  id:
    description: A resolvable URI where this credential is stored. Note that the URI contains the GS1-8 Prefix.
    type: string
  name:
    type: string
  description:
    type: string
  issuanceDate:
    type: string
  expirationDate:
    type: string
  issuer:
    description: Resolvable DID for GS1 Global Office.
    type: string
  credentialSubject:
    title: Subject Organization
    descriptions: Allocated a GS1 Member Organisation.
    type: object
    properties:
      type:
        type: array
        readOnly: true
        const:
          - Organization
        default:
          - Organization
        items:
          type: string
          enum:
            - Organization
      id:
        description: The DID for the GS1 Member Organization, or GS1 Global Office itself, to which the GS1-8 Prefix has been licensed.
        type: string
      licenceValue:
        description: The GS1-8 Prefix.
        type: string
  credentialStatus:
    type: object
  proof:
    $ref: ../snippets/proof.yml
additionalProperties: false
required:
  - '@context'
  - type
  - issuanceDate
  - issuer
  - credentialSubject
