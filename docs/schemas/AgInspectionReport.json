      "description": "Information on the inspection facility.",
      "$ref": "https://w3id.org/traceability/schemas/Place.json"
    },
    "inspector": {
      "$comment": "{\"term\": \"inspector\", \"@id\": \"https://www.lawinsider.com/dictionary/food-inspector\"}",
